

   <div class="question-container">
    <h2>Top Questions</h2>
     <%@questions.each do |question|%>
      <div class="question">
        <a href="/questions/<%=question.id%>"><%=question.title%></a> By: <a href="users/<%=User.find(question.user_id).id%>"><%=User.find(question.user_id).username%><br>

      </div>
     <%end%>
     <div>
      <% if current_user %>
      <a href="/questions/new">Create a new question</a>
      <% end %>
     </div>
   </div>

